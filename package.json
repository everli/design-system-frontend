{
  "name": "everli-design-system-frontend",
  "version": "0.1.17",
  "private": true,
  "scripts": {
    "build:common": "node ./node_modules/.bin/vue-cli-service build --target lib --name everli --dest dist ./src/components/index.js",
    "build:library": "LIBRARY=1 node ./node_modules/.bin/vue-cli-service build --target lib --name everli --dest dist/library ./src/components/index.js",
    "build": "npm run build:common && npm run build:library",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "version": "node ./scripts/version.js",
    "icons": "node ./node_modules/gulp/bin/gulp.js inline --gulpfile ./scripts/icons.js && npm run lint"
  },
  "main": "./dist/everli.common.js",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue": "^6.4.19",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.6.5",
    "css-loader": "^5.2.6",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-extname": "^0.2.2",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-to-json": "^1.0.1",
    "gulp-wrap": "^0.15.0",
    "node-sass": "^7.0.3",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "simple-git": "^3.5.0",
    "style-loader": "^2.0.0",
    "style-resources-loader": "^1.4.1",
    "vue": "^2.6.14",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.14"
  }
}
